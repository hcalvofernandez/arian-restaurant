cancel(){if(this.selfOrder.config.self_ordering_mode==="kiosk"){this.dialog.add(CancelPopup,{title:_t("Cancel order"),confirm:()=>{this.selfOrder.cancelOrder();},});}else{this.selfOrder.cancelOrder();}}
